ement.children[\"0\"].className = 'fadeIn'\n                    }}>\n                    <div>\n                      <img src={item.thumbnail} alt='thumbnail' />\n                      <video loop muted autoPlay >\n                        <source src={item.video_header} type=\"video/webm\"></source>\n                        <source src={item.video_header} type=\"video/ogg\"></source>\n                        <source src={item.video_header} type=\"video/mp4\"></source>\n                      </video>\n                      <div> </div>\n                      <section>\n                        <h1 style={{ filter: `drop-shadow(1px 1px 0px ${item.colour})` }}>{item.title}</h1>\n                      </section>\n                    </div>\n\n                    <div>\n                      <aside>\n                        <JsxParser\n                          components={{ Paragraph }}\n\n                          jsx={item.content ? item.content.split(/\\/n/g).slice(0, 4).join('') : ''}\n                        />\n                      </aside>\n                    </div>\n                  </article>\n                </a>)\n            })}</>)\n            :\n            (<></>)\n          }\n        </div>\n      </main>\n    </>)\n}",["146","147"],"/home/jayden/Documents/Website/website/website/src/components/NavBar.jsx",["148","149"],"import { React, useState } from 'react';\nimport \"./stylesheets/NavBar.css\";\nimport image from './images/Banner.png'\nimport ParticlesBg from 'particles-bg';\n\nexport default function NavBar(props) {\n\n  const [drop, setDrop] = useState(false)\n\n  return (\n    <div className='navBar'>\n      <a id='shadow' href='/'></a>\n      <div id='navParticles'>\n        <ParticlesBg num={5} color='#252525' type=\"custom\" bg={true} config={{\n          num: [2, 4],\n          rps: 0.1,\n          radius: [5, 40],\n          life: [1.5, 3],\n          v: [1, 2],\n          tha: [-40, 40],\n          alpha: [0.6, 0],\n          scale: [.1