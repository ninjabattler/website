mport avatar2 from './images/userAvatars/Mask 2.png'\nimport avatar3 from './images/userAvatars/Mask 3.png'\nimport avatar4 from './images/userAvatars/Mask 4.png'\nimport avatar5 from './images/userAvatars/Mask 5.png'\n\nconst avatars = {\n  avatar1,\n  avatar2,\n  avatar3,\n  avatar4,\n  avatar5\n}\n\nexport default function Comment(props){\n\n  // const [closed, setClosed] = useState(true)\n\n  return (\n  <article className='comment'>\n    <img className='avatar' src={avatars[`avatar${props.avatar}`]} style={{filter: `drop-shadow(2px 2px 0px ${props.pageColour || 'transparent'})`}} alt='profile pic' />\n    <header style={{textShadow: `1px 1px 0px ${props.pageColour || 'transparent'}`}}>\n      <i>Anon-{props.username}</i>\n      <p>{props.date}</p>\n    </header>\n    <span>{props.content}</span>\n  </article>)\n}",["134","135"],"/home/jayden/Documents/Website/website/website/src/components/Post.jsx",["136","137","138","139","140"],"/home/jayden/Documents/Website/website/website/src/components/PostShadow.jsx",[],["141","142"],"/home/jayden/Documents/Website/website/website/src/components/InfoBar.jsx",[],"/home/jayden/Documents/Website/website/website/src/components/Quote.jsx",[],["143","144"],"/home/jayden/Documents/Website/website/website/src/index.js",[],["145","146"],"/home/jayden/Documents/Website/website/website/src/components/ReviewPost.jsx",["147","148","149"],"import { React, useState } from 'react';\nimport './stylesheets/ReviewPosts.css';\n// import FireText from './animatedText/FireText';\n// import IceText from './animatedText/IceText';\n// import ThunderText from './animatedText/ThunderText';\n// import EarthText from './animatedText/EarthText';\n// import JsxParser from 'react-jsx-parser';\n\nexport default function ReviewPost(props){\n\n  const [closed, setClosed] = useState(true)\n\n  //Formatt the title for the url by setting all letters to lower case and replaceing\n  //spaces with underscores\n  const splitTitle = props.title.toLowerCase().split(' ')\n  let formattedTitle = '';\n\n  splitTitle.forEach(title => {\n    formattedTitle += title + '_'\n  });\n\n  formattedTitle = formattedTitle.slice(0, -1)\n\n  return (\n    <a href={`http://ninjabattler.ca/posts/${formattedTitle}`}>\n      <article className='reviewPost'>\n        <header>\n          <h1 style={{filter: `drop-shadow(1px 1px 0px ${props.colour})`}}>{props.title}</h1>\n        </header>\n        <img src={props.thumbnail}/>\n        <footer>\n          <h3 style={{filter: `drop-shadow(1px 1px 0px ${props.colour})`}}>{props.date}</h3>\n        </footer>\n      </article>\n    </a>)\n}",["150","151"],"/home/jayden/Documents/Website/website/website/src/components/TitleCard.jsx",["152"],"/home/jayden/Documents/Website/website/website/src/components/Carousel.jsx",["153","154","155","156","157","158","159"],"import { React, useState } from 'react';\nimport './stylesheets/Carousel.css';\nimport FireText from './animatedText/FireText';\nimport IceText from './animatedText/IceText';\nimport ThunderText from './animatedText/ThunderText';\nimport EarthText from './animatedText/EarthText';\nimport JsxParser from 'react-jsx-parser';\n\nexport default function Carousel({ items }) {\n  let i = 0\n  console.log(items)\n  return (\n    <section className='carousel'>\n      <main>\n        {items.map((item) => {\n          i++\n          return (\n            <a href={`/posts/${item.title.toLowerCase().replace(' ', '_')}`} className={`carouselItem ${i > 1 ? 'hidden' : 'shown'}`}>\n              <img src={item.thumbnail} />\n              <aside>\n                <h3>{item.title}</h3>\n                <h3 style={{transform: 'rotate(-7deg)'}}>{item.formatteddate}</h3>\n                <h3>{item.category}/{item.genre}</h3>\n              </aside>\n            </a>)\n        })}\n      </main>\n      {/* <div className='carouselOptions'>\n        <input type='checkbox' />\n        <input type='checkbox' />\n        <input type='checkbox' />\n      </div> */}\n    </section>)\n}","/home/jayden/Documents/Website/website/website/src/components/HomePage.jsx",["160","161","162"],"import { React, useState, useEffect } from 'react';\nimport './stylesheets/HomePage.css'\nimport CodeBlock from './CodeBlock';\nimport robot from './images/Website robot.png'\n// import AudioPlayer from './AudioPlayer'\n\nconst QUOTES = [\n  'Welcome Chrome User'\n]\nconst SUB_QUOTES = [\n  'I hope'\n]\n\nlet i = 0\n\nexport default function HomePage(props) {\n  const quoteIndex = Math.floor(Math.random() * QUOTES.length)\n  const chosenQuote = QUOTES[quoteIndex]\n  const chosenSubQuote = SUB_QUOTES[quoteIndex]\n\n  let [displayedQuote, setDisplayedQuote] = useState('|')\n  let [displayedSubQuote, setDisplayedSubQuote] = useState('|')\n\n\n  useEffect(() => {\n    const startQuoteAnim = setTimeout(() => {\n\n      const quoteAnim = setInterval(interval, 100)\n\n      function interval() {\n        setDisplayedQuote(chosenQuote.slice(0, i) + '|')\n        setDisplayedSubQuote(chosenSubQuote.slice(0, i) + '|')\n        i++\n\n\n        if (i >= chosenQuote.length) {\n          setDisplayedQuote(chosenQuote)\n          setDisplayedSubQuote(chosenSubQuote)\n          clearInterval(quoteAnim)\n        }\n      }\n\n    }, 800)\n  }, [])\n\n  return (\n    <div id='homePage'>\n\n      <div id='homePageBackground'></div>\n\n      <div id='homePageContainer'>\n        <h1>{displayedQuote}</h1>\n        <h4>{displayedSubQuote}</h4>\n      </div>\n\n      <p>Welcome to the land of uneducated opinions and a few shades of gray! Browsing this place, you will find incredible opinions on:</p>\n\n      {/* Video game */}\n      <div className='homePageInfoDiv'>\n        <video autoPlay muted loop>\n          <source src='http://files.ninjabattler.ca/video/chaotix.mp4' type=\"video/mp4\"></source>\n        </video>\n        <p>I've invented the idea of writing words on my opinions on video games! Copyright 2021, do not steal.</p>\n      </div>\n\n      {/* Programming */}\n      <div className='homePageInfoDiv'>\n        <p>I write code! Mayhaps I will write an article on the code I've written every now and then!</p>\n        <CodeBlock code={`const Imap = require('imap');\\nrequire('dotenv').config();\\nconst imap = new Imap({\\n  port: 993,\\n  host: 'imap.gmail.com',\\n  user: process.env.EMAIL,\\n  password: process.env.PASSWORD,\\n  tls: true\\n});\\nimap.once('ready', () => {\\n  console.log('Imap is ready to hack Nasa');\\n  imap.end();\\n})\\nimap.once('error', (err) => {\\n  console.log(err);\\n})\\nimap.once('end', (err) => {\\n  console.log('There is no Nasa');\\n})\\nimap.connect();`} language='js'/>\n      </div>\n\n      {/* Stuff */}\n      <div className='homePageInfoDiv'>\n        <video autoPlay muted loop>\n          <source src='http://storage.googleapis.com/personal-webiste/Video/0000-1200.mp4' type=\"video/mp4\"></source>\n        </video>\n        <p>And whatever else I feel like writing about, any and all criticism is encouraged! The comments exist for a reason!</p>\n      </div>\n      <img id='homePageRobot' src={robot} />\n      {/* <AudioPlayer title='Nights and Reala' pageColour='#FFFF00' src='https://vgmsite.com/soundtracks/nights-into-dreams-full-selection/nrivivhx/14%20-%20%28reala%29%20%28nights%20%26%20reala%29.mp3' /> */}\n\n    </div>\n  )\n}","/home/jayden/Documents/Website/website/website/src/components/AudioPlayer.jsx",[],"/home/jayden/Documents/Website/website/website/src/components/VideoHeader.jsx",[],"/home/jayden/Documents/Website/website/website/src/components/animatedText/IceText.jsx",[],"/home/jayden/Documents/Website/website/website/src/components/PostsPage.jsx",["163","164"],"/home/jayden/Documents/Website/website/website/src/App.js",[],"/home/jayden/Documents/Website/website/website/src/components/animatedText/EarthText.jsx",[],"/home/jayden/Documents/Website/website/website/src/components/animatedText/ThunderText.jsx",[],"/home/jayden/Documents/Website/website/website/src/components